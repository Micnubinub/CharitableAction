<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>


    <declare-styleable name="ParallaxScroll">
        <attr name="parallax_factor" format="float" />
        <attr name="alpha_factor" format="float" />
        <attr name="inner_parallax_factor" format="float" />
        <attr name="parallax_views_num" format="integer" />
        <attr name="circular_parallax" format="boolean" />
    </declare-styleable>

    <string name="define_parallaxscroll"></string>
    <string name="library_parallaxscroll_author">Nir Hartmann</string>
    <string name="library_parallaxscroll_authorWebsite">https://github.com/nirhart</string>
    <string name="library_parallaxscroll_libraryName">parallaxscroll</string>
    <string name="library_parallaxscroll_libraryDescription">Parallax ListView and ScrollView for Android.</string>
    <string name="library_parallaxscroll_libraryVersion">1.0</string>
    <string name="library_parallaxscroll_libraryWebsite">https://github.com/nirhart/ParallaxScroll</string>
    <string name="library_parallaxscroll_licenseId">mit</string>
    <string name="library_parallaxscroll_isOpenSource">true</string>
    <string name="library_parallaxscroll_repositoryLink">https://github.com/nirhart/ParallaxScroll.git</string>
</resources>